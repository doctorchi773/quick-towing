              "margin-bottom": "0.625rem"
            },
            ".card": {
              "& .circle-wrap": {
                "position": "relative",
                "display": "flex",
                "justify-content": "center",
                "align-items": "center",
                "max-width": "200px",
                "max-height": "200px",
                "margin-bottom": "1.25rem",
                "& svg": {
                  "& path": {
                    "stroke": "fadeOut(@circleColor, 70%)"
                  },
                  "& .circle-active": {
                    "stroke": "@circleColor"
                  }
                }
              },
              "& .circle-text": {
                "font-weight": "bold",
                "position": "absolute"
              }
            },
            ".card1": {
              "& .circle-wrap": {
                "& svg": {
                  "& .circle-active": {
                    "stroke-dashoffset": "(311 - ((311 * @circle1) / 100)) !important"
                  }
                },
                "& .circle-text::before": {
                  "content": "\"@{circle1}\""
                }
              }
            },
            ".card2": {
              "& .circle-wrap": {
                "& svg": {
                  "& .circle-active": {
                    "stroke-dashoffset": "(311 - ((311 * @circle2) / 100)) !important"
                  }
                },
                "& .circle-text::before": {
                  "content": "\"@{circle2}\""
                }
              }
            },
            ".card3": {
              "& .circle-wrap": {
                "& svg": {
                  "& .circle-active": {
                    "stroke-dashoffset": "(311 - ((311 * @circle3) / 100)) !important"
                  }
                },
                "& .circle-text::before": {
                  "content": "\"@{circle3}\""
                }
              }
            },
            ".card4": {
              "& .circle-wrap": {
                "& svg": {
                  "& .circle-active": {
                    "stroke-dashoffset": "(311 - ((311 * @circle4) / 100)) !important"
                  }
                },
